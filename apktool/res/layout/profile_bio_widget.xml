<?xml version="1.0" encoding="utf-8"?>
<merge android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:textAppearance="@style/ProfilePrimaryText" android:textSize="@dimen/text_16" android:gravity="center" android:id="@id/profile_bio_override" style="@style/ProfileBioLayout" />
    <LinearLayout android:id="@id/hometown_container" style="@style/ProfileBioLayout">
        <ImageView android:layout_gravity="top" android:paddingRight="@dimen/span16" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_home" />
        <TextView android:textSize="@dimen/text_16" android:id="@id/hometown_text" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/ProfilePrimaryText" />
    </LinearLayout>
    <LinearLayout android:id="@id/favorite_music_container" style="@style/ProfileBioLayout">
        <ImageView android:layout_gravity="top" android:paddingRight="@dimen/span16" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_music" />
        <TextView android:textSize="@dimen/text_16" android:id="@id/favorite_music_text" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/ProfilePrimaryText" />
    </LinearLayout>
    <LinearLayout android:id="@id/about_container" android:layout_marginBottom="@dimen/span4" style="@style/ProfileBioLayout">
        <ImageView android:layout_gravity="top" android:paddingRight="@dimen/span16" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_profile" />
        <TextView android:textSize="@dimen/text_16" android:id="@id/about_text" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/ProfilePrimaryText" />
    </LinearLayout>
    <LinearLayout android:id="@id/powered_by_waze_container_view" android:visibility="gone" style="@style/ProfileBioLayout">
        <ImageView android:layout_gravity="top" android:paddingRight="@dimen/span16" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_waze" />
        <TextView android:textSize="@dimen/text_16" android:id="@id/powered_by_waze_text_view" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/ProfilePrimaryText" />
    </LinearLayout>
</merge>